            //Класс not-visited добавляется тогда,когда _id юзера нету в массиве visited данного таска
            h3.emptyTasks {{vm.emptyTasks}}
            .task(ng-repeat='task in vm.tasks | filter: vm.neededTask', ng-class="{'task__not-visited':(!(task.visited.indexOf(vm._id)>-1) && vm.selector!='all')&&task.creator._id!=vm._id}")
                input.m-checkbox(ng-click='$event.stopPropagation();', type="checkbox", ng-model='item.checked', value="")
                .task__star.m-checkbox-start.m-checkbox-start-normal(
                ng-class="{'m-checkbox-checked-start':task.favourite.indexOf(vm._id)>-1}",
                ng-click="vm.addToFavourites(task)",
                ng-hide="vm.selector=='all'")
                    md-tooltip.task__star__hint(md-direction="bottom") Добавить в избранные

                a.task__name(ui-sref="newTask({taskId:task._id})") {{task.name | limitTo:20}}
                        span(ng-show="task.name.length>25") ...
                        span.task__circle(ng-hide="vm.selector=='all'||task.creator._id==vm._id" ng-class="{'task__circle-active':!(task.visited.indexOf(vm._id)>-1)}")

                .task__right-div
                    span(ng-show="task.deadline").task__deadline до:
                        span.task__deadline__date {{task.deadline | date : "dd MMMM | H:00"}}

                    .task__creator-responsible
                        span.task__creator-responsible__creator Постановщик: {{task.creator.name}}
                        span.task__creator-responsible__responsible Ответсвенный: {{task.responsible.name}}


